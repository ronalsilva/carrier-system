{
	"name": "carriers-microservice",
	"version": "0.0.1",
	"type": "module",
	"main": "index.js",
	"author": "Ronald Junger",
	"description": "Carriers Microservice",
	"scripts": {
		"dev": "tsx watch src/app.ts",
		"test": "jest --coverage --forceExit --detectOpenHandles --maxWorkers=10",
		"start": "tsx src/app.ts",
		"build": "tsc --skipLibCheck",
		"db": "npx prisma db push && npx prisma generate",
		"db:generate": "npx prisma generate",
		"db:push": "npx prisma db push",
		"docker": "docker-compose up -d"
	},
	"dependencies": {
		"@fastify/cors": "^10.1.0",
		"@fastify/jwt": "^9.1.0",
		"@fastify/swagger": "^9.5.1",
		"@fastify/swagger-ui": "^5.2.3",
		"@prisma/client": "^6.9.0",
		"dotenv": "^16.6.1",
		"fastify": "^5.4.0",
		"jsonwebtoken": "^9.0.2",
		"mongodb": "^7.1.0",
		"ts-jest": "^29.2.5"
	},
	"devDependencies": {
		"@types/jest": "^30.0.0",
		"@types/jsonwebtoken": "^9.0.10",
		"@types/node": "^22.15.32",
		"prisma": "^6.9.0",
		"tsconfig-paths": "^4.2.0",
		"tsx": "^4.19.0"
	}
}
